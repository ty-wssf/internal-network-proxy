<?xml version="1.0" encoding="UTF-8" ?>
<view x:extends="_gen/_ProxyPortMapping.view.xml" x:schema="/nop/schema/xui/xview.xdef" xmlns:x="/nop/schema/xdsl.xdef">

    <grids>
        <grid id="list"/>
        <grid id="pick-list"/>
    </grids>

    <forms>
        <form id="view"/>
        <form id="edit">
            <cells>
                <cell id="serverPort">
                    <gen-control>
                        <input-number>
                            <validations>
                                <isInt>true</isInt>
                                <minimum>0</minimum>
                                <maximum>65535</maximum>
                            </validations>
                        </input-number>
                    </gen-control>
                </cell>
                <cell id="clientPort">
                    <gen-control>
                        <input-number>
                            <validations>
                                <isInt>true</isInt>
                                <minimum>0</minimum>
                                <maximum>65535</maximum>
                            </validations>
                        </input-number>
                    </gen-control>
                </cell>
                <cell id="enabled">
                    <visibleOn>false</visibleOn>
                </cell>
            </cells>
        </form>
    </forms>

    <pages>
        <crud name="main"/>
        <picker name="picker"/>
    </pages>
</view>
